{
  "name": "leetcode-roulette-api",
  "version": "0.0.1",
  "description": "API library for https://api.leetcoderoulette.com",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leetcode-roulette/leetcode-roulette-api.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib/**/*"
  ],
  "author": "Leetcode Roulette",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leetcode-roulette/leetcode-roulette-api/issues"
  },
  "release": {
    "branches": ["master"]
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/leetcode-roulette/leetcode-roulette-api#readme",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "babel-jest": "^29.0.3",
    "dotenv": "^16.0.2",
    "jest": "^29.0.3",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "axios": "^1.6.4"
  },
  "engines": {
    "npm": ">=5.5.0",
    "node": ">=9.3.0"
  }
}
